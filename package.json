{
  "name": "@devo/browser-sdk",
  "version": "1.3.0",
  "description": "Devo browser SDK",
  "author": "Devo Dev Team",
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    }
  },
  "main": "index.js",
  "scripts": {
    "test": "mocha --reporter list --timeout 10000 test/*.js",
    "posttest": "npm run lint",
    "lint": "eslint '**/*.js'",
    "manualtest": "mocha --reporter list --timeout 10000 test/manual/*.js",
    "postinstall": "mkdir -p dist",
    "webpack:prod": "webpack --config ./webpack/webpack.prod.js",
    "webpack:dev": "webpack-dev-server --watch-content-base -d --progress --config ./webpack/webpack.dev.js"
  },
  "keywords": [
    "Devo",
    "SDK",
    "API",
    "Query",
    "LinQ"
  ],
  "dependencies": {
    "@devo/js-helper": "^1.3.0",
    "node-fetch": "2.0.0",
    "oboe": "2.1.4",
    "partial-json-parser": "^1.2.2"
  },
  "devDependencies": {
    "browserify": "^12.0.1",
    "eslint": "^4.5.0",
    "mocha": "^3.5.0",
    "mocha-each": "^1.2.0",
    "should": "^12.0.0",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.1.2",
    "css-loader": "^1.0.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.23.1",
    "uglify-es": "3.3.9",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "@babel/core": "^7.1.2",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "regenerator-runtime": "^0.11.1"
  },
  "license": "MIT"
}
